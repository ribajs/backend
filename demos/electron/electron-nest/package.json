  "version": "2.0.0-alpha.0",
  "description": "",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "prebuild:main": "rimraf dist",
    "build:riba": "webpack --progress --color --env production --config webpack.renderer.js",
    "build:main": "webpack --progress --color --env production --config webpack.main.js",
    "build": "npm run clear && npm run build:main && npm run build:riba",
    "watch:main": "webpack --progress --color --env development --config webpack.main.js --watch ",
    "watch:electron": "wait-on ./dist/main/main.js && npm run start",
    "watch": "npm run clear && run-p watch:main watch:electron",
    "start": "electron dist/main/main.js",
    "clear": "rm -rf ./dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.cts,.mts --fix && tsc --noEmit",
    "test": "jest"
  },
