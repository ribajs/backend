{
  "name": "@ribajs/node-ssr-simple-example",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn run build:theme && yarn run start:cli && yarn run start:node",
    "build:theme": "webpack --progress --color --env production",
    "build:theme:dev": "webpack --progress --color --env development",
    "build:theme:ssr": "webpack --progress --color --env production --config webpack.ssr.js",
    "build:theme:ssr:dev": "webpack --progress --color --env development --config webpack.ssr.js",
    "build:theme:csr": "webpack --progress --color --env production --config webpack.csr.js",
    "build:theme:csr:dev": "webpack --progress --color --env development --config webpack.csr.js",
    "watch": "webpack --color --watch --env development",
    "watch:prod": "webpack --color --watch --env production",
    "lint": "eslint . --ext .ts,.tsx --fix && npm run lint:ssr && npm run lint:csr",
    "lint:ssr": "tsc -p tsconfig.ssr.json --noEmit",
    "lint:csr": "tsc -p tsconfig.json --noEmit",
    "start:cli": "echo \"\nNode SSR CLI Example:\"; ssr render --component=hello-ssr-page --source-file-dir=./assets/ssr --template-dir=. --pretty=true",
    "start:node": "echo \"\nNode SSR API Example:\"; ts-node --project tsconfig.api.json api.ts",
    "start:deno": "echo \"\nDeno SSR API Example:\";deno run --config ./deno.json --allow-env --allow-run deno-api.ts"
  },
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "license": "AGPL",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-export-default-from": "^7.16.7",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@babel/plugin-transform-classes": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.2",
    "@babel/runtime-corejs3": "^7.17.2",
    "@graphql-tools/webpack-loader": "^6.6.1",
    "@ribajs/babel-config": "workspace:^",
    "@ribajs/eslint-config": "workspace:^",
    "@ribajs/tsconfig": "workspace:^",
    "@ribajs/webpack-config": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/experimental-utils": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@typescript-eslint/typescript-estree": "^5.14.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-array-includes": "^2.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "html-loader": "^3.1.0",
    "postcss-preset-env": "^7.4.2",
    "prettier": "2.5.1",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "sass": "^1.49.9",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@ribajs/core": "workspace:^",
