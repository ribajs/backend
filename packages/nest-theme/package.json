{
  "name": "@ribajs/nest-theme",
  "version": "2.0.0-alpha.0",
  "description": "Nest.js module to build Riba.js applications",
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "browser": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "scripts": {
    "clear": "rimraf dist",
    "build": "yarn run lint && yarn run build:esm && yarn run build:cjs && yarn run build:types",
    "build:types": "tsc --project ./tsconfig.json --declaration true --emitDeclarationOnly --outDir dist/types/",
    "build:esm": "tsc --project ./tsconfig.json --declaration false --module esnext --outDir dist/esm/",
    "build:cjs": "tsc --project ./tsconfig.json --declaration false --module commonjs --outDir dist/cjs/",
    "watch": "yarn run build -- --watch",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern '*.spec.ts' --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/platform-express": "^8.4.0",
    "@ribajs/core": "workspace:^",
    "@ribajs/events": "workspace:^",
    "@ribajs/node-ssr": "workspace:^",
    "@ribajs/ssr": "workspace:^",
    "@types/consolidate": "^0.14.1",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.14",
    "cache-manager": "^3.6.0",
    "cheerio": "^1.0.0-rc.10",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "jsdom": "^19.0.0",
    "node-fetch": "^2.6.7",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "whatwg-encoding": "^2.0.0",
    "yaml": "next"
  },
  "peerDependencies": {
    "pug": "*",
    "tinyliquid": "^0.x.x",
    "twig": "^1.x.x"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.2",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.0",
    "@types/cache-manager": "^3.4.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-config": "^27.5.1",
    "jest-extended": "^2.0.0",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
